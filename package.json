{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "wechat-api",
    "description": "api documentation for [wechat-api (v1.32.0)](https://github.com/node-webot/wechat-api#readme) [![npm package](https://img.shields.io/npm/v/npmdoc-wechat-api.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-wechat-api) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-wechat-api.svg)](https://travis-ci.org/npmdoc/node-npmdoc-wechat-api)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha",
        "wechat-api": "*"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-wechat-api",
    "keywords": [
        "documentation",
        "wechat-api"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_wechat_api.js",
    "name": "npmdoc-wechat-api",
    "nameAlias": "npmdoc_wechat_api",
    "nameOriginal": "npmdoc-wechat-api",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-wechat-api"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.1"
